{
  "parser": "@typescript-eslint/parser",
  "extends": ["airbnb"],
  "rules": {
    "indent": ["error", 2],
    "max-len": [
      1, 70, 2,
      {
        "ignoreComments": true
      }
    ],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx",".ts"] }],
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off",
    "import/extensions": "off"
  }
}
